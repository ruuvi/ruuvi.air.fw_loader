/ {
	gpio_enable_sensors: gpio_enable_sensors {
		compatible = "power-switch";
		status = "okay";
		gpios = <&gpio0 11 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>;
	};
};

/ {
	chosen {
			nordic,pm-ext-flash = &mx25r64;
	};
};

/ {
    aliases {
        button-pinhole = &button0;
        gpio-supercap-active = &button1;
        led-green = &led0;
        led-red = &led1;
    };
};

